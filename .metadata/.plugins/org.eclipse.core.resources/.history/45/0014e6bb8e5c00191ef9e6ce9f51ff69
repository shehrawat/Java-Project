package ObjectClonning;

public class Clonning 
{

	public static void main(String[] args) 
	{
		Abc obj = new Abc();
		obj.i = 5;  // -> i = 5
		obj.j = 6;	// -> j = 6
	
		Abc obj1 = obj.clone();
		
		System.out.println(obj1);
		System.out.println(obj);
	}

}

class Abc
{
	int i;
	int j;
	
	@Override
	public String toString()
	{
		return "Abc{" + "i=" + i + ", j=" + j + '}';
		
	}
}
