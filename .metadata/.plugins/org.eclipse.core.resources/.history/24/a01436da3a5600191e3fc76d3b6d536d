package SuperKeyword;

public class Super3 {

	public static void main(String[] args) 
	{

	}

}

class A3		// By default every class extends Object class
{
	int i =5;
	public void abc()
	{
		System.out.println("A1 abc");
	}
}

class B3 extends A3
{
	public void abc()
	{
		int i=4;
//		super.abc();
		System.out.println(super.i);
	}
}