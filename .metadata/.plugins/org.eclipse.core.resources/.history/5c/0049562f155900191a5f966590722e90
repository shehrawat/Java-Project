package Interface;

public class Demo implements P
{
	public void show()
	{
		System.out.println("Hello");
	}

	public static void main(String[] args) 
	{
		Demo obj = new Demo();
		
		if(obj instanceof P)
			obj.show();
		else
			System.out.println("No Permision");
	}

}

interface P		// This is Marker Interface because it has no any method
{
	
}
