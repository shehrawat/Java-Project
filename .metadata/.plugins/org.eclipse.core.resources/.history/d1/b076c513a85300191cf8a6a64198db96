package Object;

public class PassObjectInParameter 
{

	public static void main(String[] args) 
	{
		Paper p = new Paper();
		p.setText("Hello World");
		p.getText();
	}
}

class Printer
{
	public void print()
	{
		System.out.println("Hi");
		//p.setText("Get Lost");
	}
}

class Paper
{
	String text;
	public void setText(String t)
	{
		text = t;
	}
	
	public String getText()
	{
		return text;
	}
}
