package SuperKeyword;

public class Super2 
{

	public static void main(String[] args) 
	{
		B1 obj = new B1();
		obj.abc();
	}

}

class A1		// By default every class extends Object class
{
	public void abc()
	{
		System.out.println("A1 abc");
	}
}

class B1 extends A1
{
	public void abc()
	{
		super.abc();
		System.out.println("B1 abc");
	}
}
