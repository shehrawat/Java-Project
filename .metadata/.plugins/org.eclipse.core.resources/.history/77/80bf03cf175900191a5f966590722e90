package String;

import java.util.Scanner;

public class StringOp 
{

	public static void main(String[] args) 
	{
		String s1,s2;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter two Strings : ");
		s1 = sc.nextLine();
		s2 = sc.nextLine();
		
		String s3 = s1.concat(s2);
	}

}
