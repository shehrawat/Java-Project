package SuperKeyword;

public class Super2 
{

	public static void main(String[] args) 
	{
		B1 obj = new B1();
		obj.show();
	}

}

class A1		// By default every class extends Object class
{
	int i = 5;
	public void abc()
	{
		System.out.println("A1 abc");
	}
}

class B1 extends A1
{
	int i = 4;
	public void show()
	{
		System.out.println(super.i);
	}
	
	public void abc()
	{
		System.out.println("B1 abc");
	}
}
